std: namespace = {
    println: (args...) = {
        (std::cout << ... << args) << "\n";
    }
}

hello: (msg: std::string_view) = {
    std::cout << "Hello, (msg)$!\n";
    std::println(msg);
}

main: () = {
    words: std::vector = ( "Alice", "Bob" );
    hello( words[0] );
    hello( words[1] );
}
